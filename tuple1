
imelda = "More Mayhem", "Imelda May", 2011,(

    (1, "Pulling the rug"), (2, "Psycho"), (3, "Mayhem"), (4, "Kentish Town Waltz"))

print(imelda)

title, artist, year, tracks = imelda


for song in tracks:
    track, title = song
    # print(song)
    print(f"\tTrack number {track}, Title {title}")
